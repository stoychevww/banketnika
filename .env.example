# Banketnika Discord Bot Configuration
# Copy this file to .env and fill in your values

# Discord Bot Token (REQUIRED)
# Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_bot_token_here

# Bot Configuration
BOT_PREFIX=!
BOT_DESCRIPTION=Advanced Bulgarian Music Bot with YT-DLP & Enhanced Bot Detection Evasion

# Logging
LOG_LEVEL=INFO
DEBUG=False

# Music Settings
DEFAULT_VOLUME=0.5
MAX_VOLUME=1.0
QUEUE_LIMIT=50

# YouTube Settings
YOUTUBE_COOKIES_FILE=
YOUTUBE_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Database (if using one in the future)
DATABASE_URL=

# Optional: Webhook URLs for logging
ERROR_WEBHOOK_URL=
INFO_WEBHOOK_URL=

# Optional: Custom embed colors (hex codes)
EMBED_COLOR_SUCCESS=0x00ff00
EMBED_COLOR_ERROR=0xff0000
EMBED_COLOR_INFO=0x0099ff

# Optional: Performance settings
MAX_CONCURRENT_DOWNLOADS=3
DOWNLOAD_TIMEOUT=30

# Optional: Feature flags
ENABLE_ALTERNATIVE_PLAYER=True
ENABLE_BUTTON_CONTROLS=True
ENABLE_QUEUE_LOOP=True 